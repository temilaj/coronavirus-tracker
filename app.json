{
  "expo": {
    "name": "Coronavirus tracker Nigeria",
    "slug": "coronavirus-tracker-nigeria",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.temilajumoke.covidTracker",
      "buildNumber": "0.5.0"
    },
    "android": {
      "package": "com.temilajumoke.covidTracker",
      "versionCode":  2,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_PHONE_STATE",
        "READ_CONTACTS"
      ]
    }
  }
}
